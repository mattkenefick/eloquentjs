{
    "name": "eloquent-js",
    "version": "0.6.6",
    "description": "Laravel mock of Eloquet for Javascript",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc --skipLibCheck",
        "format": "npm run prettier:fix && npm run lint:fix",
        "lint": "eslint src --ext=ts",
        "lint:fix": "npm run lint -- --fix",
        "postversion": "git push && git push --tags",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "prettier": "prettier -c src/**/*.ts",
        "prettier:fix": "npm run prettier -- --write",
        "preversion": "npm run lint",
        "test": "ts-mocha -p tsconfig.testing.json test/**/*.spec.ts",
        "version": "npm run format && git add -A src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mattkenefick/eloquentjs.git"
    },
    "files": [
        "dist/**/*"
    ],
    "author": "Polymer Mallard LLC",
    "license": "ISC",
    "dependencies": {
        "@types/chai": "^4.2.11",
        "@types/lodash": "^4.14.155",
        "@types/node": "^13.13.9",
        "@types/node-fetch": "^2.5.7",
        "chai": "^4.2.0",
        "dispatcher": "github:mattkenefick/dispatcher",
        "https": "^1.0.0",
        "mocha": "^7.2.0",
        "node-fetch": "^2.6.0",
        "ts-node": "^8.10.2",
        "tslint": "^6.1.3",
        "typescript": "^3.9.3"
    },
    "devDependencies": {
        "@types/expect": "^24.3.0",
        "@types/mocha": "^7.0.2",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "eslint": "^7.25.0",
        "prettier": "^2.2.1",
        "ts-mocha": "^7.0.0",
        "tslib": "^2.2.0"
    }
}
